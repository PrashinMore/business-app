  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:release": "npm run clean:all && npx expo prebuild --platform android --no-install && npx expo run:android --variant release",
    "android:release:full": "npm run clean:all && npx expo prebuild --clean && npx expo run:android --variant release",
    "android:bundle": "npx expo prebuild --platform android --no-install && cd android && gradlew.bat bundleRelease && cd ..",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean:all": "node scripts/clean-cache.js",
    "prebuild:clean": "npx expo prebuild --clean"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.5.1",
